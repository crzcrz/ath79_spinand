		spmi_bus: qcom,spmi@200f000 {
			compatible = "qcom,spmi-pmic-arb";
			reg = <0x200f000 0x1000>,
				<0x2400000 0x800000>,
				<0x2c00000 0x800000>,
				<0x3800000 0x200000>,
				<0x200a000 0x000700>;
			reg-names = "core", "chnls", "obsrvr", "intr", "cnfg";
			interrupt-names = "periph_irq";
			interrupts = <GIC_SPI 190 IRQ_TYPE_NONE>;
			qcom,ee = <0>;
			qcom,channel = <0>;
			#address-cells = <2>;
			#size-cells = <0>;
			interrupt-controller;
			#interrupt-cells = <4>;
			cell-index = <0>;
		};

		i2c_1: i2c@78b7000 {
			compatible = "qcom,i2c-qup-v2.2.1";
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0x78b7000 0x600>;
			interrupts = <GIC_SPI 97 0x4>;
			clocks = <&gcc GCC_DUMMY_CLK>,
				<&gcc GCC_DUMMY_I2C_CLK>;
			clock-names = "iface", "core";
			clock-frequency  = <100000>;
			dmas = <&blsp_dma 17>, <&blsp_dma 16>;
			dma-names = "rx", "tx";

			qca_codec: qca_codec@12 {
				compatible = "qca,ipq8074-codec";
				reg = <0x12>;
				status = "ok";
			};

		};

		audio: audio@7700000 {
			compatible = "qca,ipq8074-audio-adss";
			reg = <0x7700000 0x34>;
			resets = <&gcc GCC_ADSS_BCR>;
			reset-names = "blk_rst";
			status = "disabled";
		};

		mbox0: mbox@7708000 {
			dma-index = <0>;
			compatible = "qca,ipq8074-mbox";
			reg = <0x7708000 0x1000>;
			interrupts = <0 24 0>;
			tx-channel = <MBOX0_TX_ID>;
			rx-channel = <MBOX0_RX_ID>;
			status = "disabled";
		};

		mbox3: mbox@770e000 {
			dma-index = <3>;
